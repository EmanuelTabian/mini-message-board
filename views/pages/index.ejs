<body>
    <h1><%= title %></h1>

    <ul class="messages">
        <% messages.forEach(msg => { %>
            <li>
                <p><%= msg.msg %></p> by
                <span><%= msg.user %></span>
            </li>
        <% }) %>
    </ul>
    
    <%- include('../partials/form', { title: 'Add a Message' }) %>
</body>